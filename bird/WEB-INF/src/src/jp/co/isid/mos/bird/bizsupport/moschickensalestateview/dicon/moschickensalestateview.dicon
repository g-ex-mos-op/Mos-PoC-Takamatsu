	</component>
	<component name="uiDayMenuInfoDao" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.dao.UIDayMenuInfoDao">
        <aspect>dao.interceptor</aspect>
	</component>
	<component name="uiSaleStateInfoDao" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.dao.UISaleStateInfoDao">
        <aspect>dao.interceptor</aspect>
	</component>

	<!-- LOGIC -->
	<component name="getShohinGroupInfoLogic" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.logic.impl.GetShohinGroupInfoLogicImpl"/>
	<component name="getTitleInfoLogic" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.logic.impl.GetTitleInfoLogicImpl"/>
	<component name="getMiseInfo" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.logic.impl.GetMiseInfoLogicImpl"/>
