	<component name="mosChickenSaleStateViewCondAction" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.action.impl.MosChickenSaleStateViewConditionActionImpl" instance="request">
		<property name="mosChickenSaleStateViewDto">mosChickenSaleStateViewDto</property>
	</component>
	<component name="mosChickenSaleStateViewResultAction" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.action.impl.MosChickenSaleStateViewResultActionImpl" instance="request">
		<property name="mosChickenSaleStateViewDto">mosChickenSaleStateViewDto</property>
	</component>	

