	<component class="jp.co.isid.mos.bird.framework.control.BirdUserInfo" instance="session"/>
	<component class="jp.co.isid.mos.bird.framework.control.BirdDateInfo" instance="session"/>

	<!-- DAO -->
	<component name="uiTitleInfoDao" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.dao.UITitleInfoDao">
        <aspect>dao.interceptor</aspect>
	</component>
	<component name="uiShohinGroupInfoDao" class="jp.co.isid.mos.bird.bizsupport.moschickensalestateview.dao.UIShohinGroupInfoDao">
