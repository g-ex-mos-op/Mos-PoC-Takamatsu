	<component name="trnPLInfoSec" class="jp.co.isid.mos.bird.bizsupport.lumptakein.entity.TrnPLInfo" instance="session"/>
	<component name="trnZenPLInfoSec" class="jp.co.isid.mos.bird.bizsupport.plregist.entity.TrnZenPLInfo" instance="session"/>

	<!-- OTHER -->
	<component class="jp.co.isid.mos.bird.framework.control.BirdUserInfo" instance="session"/>
	<component class="jp.co.isid.mos.bird.framework.control.BirdDateInfo" instance="session"/>

	<!-- ACTION -->
	<component name="plContentFormAction" class="jp.co.isid.mos.bird.bizsupport.plcontentconfirm.action.impl.PlContentFormActionImpl" instance="request"/>
	<component name="plContentConfirmAction" class="jp.co.isid.mos.bird.bizsupport.plcontentconfirm.action.impl.PlContentConfirmActionImpl" instance="request"/>
	
	<!-- CONVERTER -->
	<component name="dateConverterPlRegist" class="jp.co.isid.mos.bird.framework.text.converter.DateConverter" instance="request">
		<arg>@jp.co.isid.mos.bird.framework.util.formatter.DateFormatter@DATE_TYPE_YM</arg>
		<arg>@jp.co.isid.mos.bird.framework.util.formatter.DateFormatter@PATTERN_MONTH_SLASH</arg>
	</component>
	<component name="dateConverter2PlRegist" class="jp.co.isid.mos.bird.framework.text.converter.DateConverter" instance="request">
		<arg>@jp.co.isid.mos.bird.framework.util.formatter.DateFormatter@DATE_TYPE_YM</arg>
		<arg>@jp.co.isid.mos.bird.framework.util.formatter.DateFormatter@PATTERN_MONTH_KANJI_YM</arg>
	</component>

	<component name="changeLineEditConverter" class="jp.co.isid.mos.bird.framework.text.converter.ChangeLineEditConverter" instance="request"/>
	<component name="changeLineRefConverter" class="jp.co.isid.mos.bird.framework.text.converter.ChangeLineRefConverter" instance="request"/>

	<component name="pdfAction" class="jp.co.isid.mos.bird.framework.action.impl.PdfOutputActionImpl" instance="request">
		<property name="pdfOutputDto">plContentConfirmDto</property>
		<property name="pdfOutputLogic">PlContentConfirmPdfDLLogic</property>
	</component>
	
	<!--component name="pdfOutputDto" class="jp.co.isid.mos.bird.bizsupport.plcontentconfirm.dto.PlContentConfirmDto" instance="session"/-->
	<component name="PlContentConfirmPdfDLLogic" class="jp.co.isid.mos.bird.bizsupport.plcontentconfirm.logic.PlContentConfirmPdfDLLogicImpl">
<!--		<property name="fileName">"PdfSample.pdf"</property> -->
	</component>
</components>
