	<component name="getPLDataLogic" class="jp.co.isid.mos.bird.bizsupport.common.logic.impl.GetPLDataLogicImpl"/>
	<component name="getZenPLDataLogic" class="jp.co.isid.mos.bird.bizsupport.common.logic.impl.GetZenPLDataLogicImpl"/>

	<!-- DTO -->
	<component name="pullDownMenuDto" class="jp.co.isid.mos.bird.framework.dto.PullDownMenuDto" autoBinding="none" instance="session"/>
	<component name="plRegistSecDto" class="jp.co.isid.mos.bird.bizsupport.common.dto.PlRegistDto" autoBinding="none" instance="session"/>
	<component name="plRegistStateDto" class="jp.co.isid.mos.bird.bizsupport.common.dto.PlRegistStateDto" autoBinding="none" instance="session"/>
	<component name="ownerSearchDto" class="jp.co.isid.mos.bird.commonform.ownersearch.dto.OwnerSearchDto" autoBinding="none" instance="session"/>
	<component name="lumpTakeInDto" class="jp.co.isid.mos.bird.bizsupport.lumptakein.dto.LumpTakeInDto" autoBinding="none" instance="session"/>

	<component name="plContentConfirmDto" class="jp.co.isid.mos.bird.bizsupport.plcontentconfirm.dto.PlContentConfirmDto" autoBinding="none" instance="session"/>

