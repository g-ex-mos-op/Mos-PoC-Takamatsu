<components namespace="plcontentconfirm">
    <include path="dao.dicon"/>
    <include path="jp/co/isid/mos/bird/common/dicon/birdCommon.dicon"/>
	<include path="jp/co/isid/mos/bird/framework/dicon/birdInterceptor.dicon"/>

	<component class="org.seasar.framework.container.autoregister.AspectAutoRegister">
	    <property name="interceptor">birdInterceptor.actionInterceptorChain</property>
	    <initMethod name="addClassPattern">
	        <arg>"jp.co.isid.mos.bird"</arg>
	        <arg>".*ActionImpl"</arg>
	    </initMethod>
	    <initMethod name="registerAll"/>
	</component> 
	<component 
	  class="org.seasar.framework.container.autoregister.AspectAutoRegister">
	    <property name="interceptor">birdInterceptor.logicInterceptorChain</property>
	    <initMethod name="addClassPattern">
	        <arg>"jp.co.isid.mos.bird"</arg>
	        <arg>".*LogicImpl"</arg>
	    </initMethod>
	    <initMethod name="registerAll"/>  
	</component>
	
	<!-- DAO -->
	<component name="msTUserOnerInfoDao" class="jp.co.isid.mos.bird.bizsupport.common.dao.MSTUserOnerInfoDao">
        <aspect>dao.interceptor</aspect>
	</component>
	<component name="msTUserMiseInfoDao" class="jp.co.isid.mos.bird.bizsupport.common.dao.MSTUserMiseInfoDao">
        <aspect>dao.interceptor</aspect>
	</component>
	<component name="trnPLAuthorInfoDao" class="jp.co.isid.mos.bird.bizsupport.common.dao.TrnPLAuthorInfoDao">
        <aspect>dao.interceptor</aspect>
	</component>
	<component name="trnPLInfoDao" class="jp.co.isid.mos.bird.bizsupport.common.dao.TrnPLInfoDao">
        <aspect>dao.interceptor</aspect>
	</component>
	<component name="trnZenPLInfoDao" class="jp.co.isid.mos.bird.bizsupport.common.dao.TrnZenPLInfoDao">
        <aspect>dao.interceptor</aspect>
	</component>
	<component name="trnPosZenUriageDao" class="jp.co.isid.mos.bird.bizsupport.common.dao.TrnPosZenUriageDao">
        <aspect>dao.interceptor</aspect>
	</component>

	<component name="trnPLLimitDao" class="jp.co.isid.mos.bird.bizsupport.lumptakein.dao.TrnPLLimitDao">
        <aspect>dao.interceptor</aspect>
	</component>
	<component name="uiGroupTogoOwnerDao" class="jp.co.isid.mos.bird.bizsupport.common.dao.UIGroupTogoOwnerDao">
