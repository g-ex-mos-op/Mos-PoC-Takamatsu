	<component name="pullDownMenuDto" class="jp.co.isid.mos.bird.framework.dto.PullDownMenuDto" autoBinding="none" instance="session"/>
	<component name="projectPlanOfferDto" class="jp.co.isid.mos.bird.entry.projectplanoffer.dto.ProjectPlanOfferDto" autoBinding="none" instance="session"/>	
	<component name="projectPlanOfferBakDto" class="jp.co.isid.mos.bird.entry.projectplanoffer.dto.ProjectPlanOfferBakDto" autoBinding="none" instance="session"/>	
	<component name="eventListDto" class="jp.co.isid.mos.bird.entry.eventlist.dto.EventListDto" autoBinding="none" instance="session"/>
	<component name="entryStaffSearchDto" class="jp.co.isid.mos.bird.entry.commonform.entrystaffsearch.dto.EntryStaffSearchDto" autoBinding="none" instance="session"/>
	
	<!-- ACTION -->
	<component name="projectPlanOfferEditAction" class="jp.co.isid.mos.bird.entry.projectplanoffer.action.impl.ProjectPlanOfferEditActionImpl" instance="request">
