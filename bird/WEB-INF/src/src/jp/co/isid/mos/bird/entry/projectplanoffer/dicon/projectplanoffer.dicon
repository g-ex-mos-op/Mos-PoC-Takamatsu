	<component name="checkOfferInputParamLogic" class="jp.co.isid.mos.bird.entry.projectplanoffer.logic.impl.CheckOfferInputParamLogicImpl"/>
	<component name="getOfferMiseInfoLogic" class="jp.co.isid.mos.bird.entry.projectplanoffer.logic.impl.GetOfferMiseInfoLogicImpl"/>
