	<component name="numericConverter" class="jp.co.isid.mos.bird.framework.text.converter.NumericConverter" instance="request"/>
	<component name="projectPlanOfferCharacterEncodingConverter" class="jp.co.isid.mos.bird.framework.text.converter.CharacterEncodingConverter" instance="request"/>
	<component name="projectPlanOfferZipConverter" class="jp.co.isid.mos.bird.framework.text.converter.MaskConverter" instance="request">
